{

  "LogRecorder": {
    /*不注册日志器*/
    "noRegist": true,
    /*是否开启标准控制台日志,动态修改无效*/
    "console": true,
    /*是否使用基础文本日志记录器,动态修改无效*/
    "innerLogger": true,
    /*是否开启跟踪日志,可动态修改*/
    "monitor": true,
    /*跟踪日志是否包含详细信息,可动态修改*/
    "details": true,
    /*是否开启SQL语句日志,可动态修改*/
    "sql": false
  },
  "Logging": {
    /*标准日志设置,参考NetCore相关内容*/
    "IncludeScopes": false,
    /*基础文本日志记录器配置*/
    "Text": {
      /*日志路径,动态修改可能延迟生效,如未设置,则会被设置为与应用数据文件夹同级的logs目录*/
      "path": null,
      /*是否按日期分文件夹,动态修改可能延迟生效*/
      "dayFolder": false,
      /*最大文件数量,可动态修改*/
      "disable": false,
      /*最大文件数量(dayFolder为false有效),可动态修改*/
      "maxFile": 99,
      "LogLevel": "Trace"
    },
    "LogLevel": {
      "Default": "Trace",
      "Microsoft": "Trace"
    },
    "Console": {
      //"Format": "Systemd"
    }
  }
}