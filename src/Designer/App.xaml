<Application x:Class="Agebull.EntityModel.Designer.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:client="clr-namespace:Agebull.Common.Mvvm;assembly=Agebull.Common.Mvvm"
             StartupUri="View/MainWindow.xaml">
    <Application.Resources>
        <ResourceDictionary>
            <client:NullVisibleConverter x:Key="NullVisibleConverter"/>
            <client:BoolToVisibleConverter x:Key="BoolToVisibleConverter"/>
            <client:ReversionVisibleConverter x:Key="ReversionVisibleConverter"/>
            <client:ReversionBoolToVisibleConverter x:Key="ReversionBoolToVisibleConverter"/>
            <client:BoolReversalConverter x:Key="BoolReversalConverter"/>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="Style\Icon.xaml"/>
                <ResourceDictionary Source="Style\Button.xaml"/>
                <ResourceDictionary Source="Style\TreeView.xaml"/>
                <ResourceDictionary Source="Style\Menu.xaml"/>
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>